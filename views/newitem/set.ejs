                    return this.is_bgmusic_on.content == this.is_bgmusic_on.back;
                },
                toggle_shareicon_disable: function(){
                    return this.shareicon.type == this.shareicon.inittype;
                },
                bgmusic_uped: function(){
                    return {
                        url: this.bgmusic.uped?this.bgmusic.uped:'http://qncdn.sz-sti.com/images/tools/785.mp3',
                        name: this.bgmusic.uped?this.bgmusic.uped.split('/')[this.bgmusic.uped.split('/').length-1].substr(-20, 20):'默认音乐'
                    }
                },
                sharedesc_disable: function(){
                    return this.sharedesc.content == this.sharedesc.back;
