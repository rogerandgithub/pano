                // info_disable: function(){
                //     return this.info.content.ad1 == this.info.back.ad1 && this.info.content.ad2 == this.info.back.ad2 && this.info.content.ad3 == this.info.back.ad3;
                // }
            },
            methods: {
                toggletip: function(name){
                    this.tipshow[name] = !this.tipshow[name];
                },
                subsceneinfo: function(){
                    if(mainWrap.checkexpire())return;
                    if(!this.menu_disable){
                        this.sub_menu();
                    }
                    var validatePhone = this.validatePhone();
                    if(!this.phone_disable&&validatePhone)this.sub_phone();

                    if(!this.prefix_disable) this.sub_prefix();
                    if(!this.sharedesc_disable) this.sub_sharedesc();
                    // if(!this.info_disable) this.subinfo();
                    if(!this.broadcast_disable) this.toggle_broadcast(1);
                    if(!this.toggle_bgmusic_disable) this.toggle_bgmusic(1);
                    // if(!this.toggle_shareicon_disable) {
                    //       if(this.uploadicon){
                    //         if(!this.shareicon.url){
                    //             toast.showmsg('请先上传小图标');
                    //             return;
                    //         }
                    //     }
                    //     this.toggle_shareicon(1);  
                    // }
                    if(!this.qrcode_disable) this.toggle_qrcode(1);
                    if(!this.rotate_disable) this.toggle_rotate(1);
                            console.log(this.uploadicon);

                    if(!this.prefix_disable || !this.menu_disable || !this.sharedesc_disable || !this.broadcast_disable || !this.toggle_bgmusic_disable || !this.toggle_shareicon_disable || (!this.phone_disable&&validatePhone) ||!this.qrcode_disable ||!this.rotate_disable ||!this.rotate_disable){
