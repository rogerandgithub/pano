        var mainWrap = new Vue({
            el:'#function-wrapper',
            data:{
                preview : false,
                uploadobj : {},
                previewurl : '', 
                maskwidth : '0%',
                iscircle: false,
                cancut: true,
                isdelete: false,
                isrender: false,
                expire:false,
                expiredate:'<%=user.expire%>',
                expireinfo: '该功能为商业版功能，开通请联系销售经理',
                url: {
                    logo: '<%=user.logo%>',
                    topad: '<%=user.trademark%>',
                    botad: '<%=user.itccontactstel%>'
                },
                uploadtype: '',
                jurisdiction:{
                    permission_logo:'<%=user.permission_logo%>',
                    permission_trademark:'<%=user.permission_trademark%>'
                },
                confirm: false,
                tipshow: {
                    logo: false,
                    topad: false,
                    botad: false
                }
            },
            computed: {
                logo_uped: function(){
                    return this.expire?'商业版功能':this.url.logo?'点击查看':'点击上传'
                },
                topad_uped: function(){
                    return this.expire?'商业版功能':this.url.topad?'点击查看':'点击上传'
                },
                botad_uped: function(){
                    return this.expire?'商业版功能':this.url.botad?'点击查看':'点击上传'
                }
            },
            methods:{
                toggletip: function(name){
                    this.tipshow[name] = !this.tipshow[name];
                },
                init: function(){
                    var now = Date.parse(new Date());
                    var expiredate = Date.parse(new Date(this.expiredate));
                    if(now>expiredate){
                        this.expire = true;
                        advanceset.shareicon.type = 0;
                    }else{
                        advanceset.shareicon.type = JSON.parse(advanceset.shareicon.uped)[0];
                        advanceset.shareicon.inittype = JSON.parse(advanceset.shareicon.uped)[0];
                        JSON.parse(advanceset.shareicon.uped)[0] != 1?advanceset.shareicon.type = JSON.parse(advanceset.shareicon.uped)[0]:'';
                        advanceset.shareicon.url = JSON.parse(advanceset.shareicon.uped)[1]?JSON.parse(advanceset.shareicon.uped)[1]:'';
                        advanceset.shareicon.initurl = JSON.parse(advanceset.shareicon.uped)[1]?JSON.parse(advanceset.shareicon.uped)[1]:'';
                        advanceset.shareicon.url?advanceset.shareicon.url = 'http://qncdn.sz-sti.com/images/shareicon/'+JSON.parse(advanceset.shareicon.uped)[1]:'';
                        advanceset.shareicon.initurl?advanceset.shareicon.initurl = 'http://qncdn.sz-sti.com/images/shareicon/'+JSON.parse(advanceset.shareicon.uped)[1]:'';
                    }
                },
                checkexpire: function(){
                    if(this.expire){
                        toast.showmsg(this.expireinfo, true);
                        return true;
                    }else{
                        return false;
                    }
                },
                upload_logo: function(event){
                    if(this.checkexpire())return;
                    this.cancut = false;
                    if(this.url.logo){
                        this.filechange(event,this.url.logo);
                        this.isdelete = true;
                    }else{
                        this.uploadtype = true;
                        $("#file_input").click();
                        this.isdelete = false;
                    }
                    this.uploadobj = {
                        type: 'logo',
                        uploadurl: '/tokenitem/logo',
                        deleteurl: '/setitem/set-logo'
                    };
                },
                upload_topad: function(event){
                    if(this.checkexpire())return;
                    this.cancut = true;
                    if(this.url.topad){
                        this.filechange(event,this.url.topad);
                        this.isdelete = true;
                    }else{
                        $("#file_input").click();
                        this.isdelete = false;
                    } 
                    this.uploadobj = {
                        type: 'topad',
                        uploadurl: '/tokenitem/topad',
                        deleteurl: '/setitem/deltrademark'
                    };
                },
                upload_botad: function(event){
                    if(this.checkexpire())return;
                    this.cancut = true;
                    if(this.url.botad){
                        this.filechange(event,this.url.botad);
                        this.isdelete = true;
                    }else{
                        $("#file_input").click();
                        this.isdelete = false;
                    }
                    this.uploadobj = {
                        type: 'botad',
                        uploadurl: '/tokenitem/botad',
                        deleteurl: '/setitem/delbottrademarks'
                    };
                },
                getObject: function(){
                    var file = document.getElementById('file_input').files[0];
                    var url = undefined;
                    if (window.createObjectURL) {
                        url = createObjectURL(file);
                    }else if(window.URL){
                        url = URL.createObjectURL(file);
                    }else if(window.webkitURL){
                        url = webkitURL.createObjectURL(file);
                    };
                    return {
                        url: url,
                        size: file.size,
                        type: file.type
                    };
                },
                filechange: function(event,src){
                    if(src){
                        var url = src;
                    }else{
                        var obj = this.getObject();
                        if(obj.type.split('/')[0]!='image'){
                            toast.showmsg('不是图片类型的文件', true);
                            return;
                        }
                        if(obj.size>307200){
                            toast.showmsg('图片大小不得大于300K', true);
                            return;
                        };
                        var url = obj.url;
                    }
                    if(this.uploadtype){
                        var _this = this;
                        var image = new Image();
                        image.src = url;
                        image.crossOrigin = "*";
                        image.onload = function(){

                            // if((image.width/image.height)>1:1){
                            //     toast.showmsg('建议上传宽高比大于或等于1:1的图片', true);
                            //     return;
                            // }
                            url?_this.preview = true:_this.preview = false;
                            _this.iscircle = false;
                            _this.previewurl = url;
                            _this.$nextTick(function(){
                                $.smoothScroll({
                                    scrollTarget: '#editheader'
                                });
                            });
                        };
                    }else{
                        url?this.preview = true:this.preview = false;
                        this.iscircle = false;
                        this.previewurl = url;
                        this.$nextTick(function(){
                            $.smoothScroll({
                                scrollTarget: '#editheader'
                            });
                        });
                    }
                },
                upload: function(){
                    var _this = this;
                    this.confirm = true;
                    $.post(this.uploadobj.uploadurl,{type: 'global'},function(res){
                        console.log(res);
                        var token = res.token;
                        var key = res.key;
                        var url = res.url;
                        Q.addEvent("progress", function(p, s) {
                            console.log(1);
                            _this.maskwidth = (p==100?99:p)+'%';
                        });
                        Q.addEvent("putFinished", function(fsize, res, taking) {
                            console.log(_this.uploadobj.type);
                            console.log(2);
                            switch(_this.uploadobj.type){
                                case 'logo':
                                    _this.url.logo = 'http://qncdn.sz-sti.com/'+key;
                                    break;
                                case 'topad':
                                    _this.url.topad = 'http://qncdn.sz-sti.com/'+url;
                                    break;
                                case 'botad':
                                    _this.url.botad = 'http://qncdn.sz-sti.com/'+url;
                                    break;
                                default:;
                            }
                            _this.hidepreview();
                            _this.isrender = false;
                            toast.showmsg('上传成功');
                        });
                        Q.SetToken(token);

                        _this.iscircle?Q.Upload_base64(_this.previewurl.replace('data:image/png;base64,',''), url):
                        Q.Upload(document.getElementById('file_input').files[0], url);
                    });
                },
                goback: function(){
                    this.url[this.uploadobj.type]?
                    this.previewurl = this.url[this.uploadobj.type]:
                    this.previewurl = this.getObject().url;
                    this.iscircle = false;
                },
                delete: function(){
                    var _this = this;
                    $.post(this.uploadobj.deleteurl,{
                            type: 'global'
                        },function(res){
                            if(res.code == 0){
                                toast.showmsg('删除成功');
                                _this.url[_this.uploadobj.type] = '';
                                _this.preview = false;
                            }else{
                                toast.showmsg(res.msg, true);
                            }
                        }
                    );
                },
                render: function(type){
                    var width = 400;
                    var _this = this;
                    var image = new Image();
                    image.crossOrigin = "*";

                    image.onload = function(){
                        var canvas = document.getElementById("canvas");
                        var ctx = canvas.getContext("2d");
                        ctx.clearRect(0, 0, image.width, image.height);

                        var max = Math.max(image.width, image.height);
                        var min = Math.min(image.width, image.height);

                        var MAX_HEIGHT = 400;
                        canvas.height = canvas.width = MAX_HEIGHT;

                        var pattern = ctx.createPattern(image, "no-repeat");
                        ctx.scale(MAX_HEIGHT/min, MAX_HEIGHT/min);

                        type == 1?ctx.rect(0, 0, min, min):ctx.arc(min/2, min/2, min/2, 0, 2 * Math.PI);

                        image.height<image.width?ctx.translate((min-max)/2, 0):ctx.translate(0, (min-max)/2);
                        ctx.fillStyle = pattern;
                        ctx.fill();  

                        _this.previewurl = canvas.toDataURL('image/png');
                        _this.iscircle = true;
                        _this.isrender = true;
                    };
                    image.src = this.previewurl;
                },
                hidepreview: function(){
                    this.preview = false;
                    this.confirm = false;
                    this.isrender = false;
                    this.maskwidth = '0%';
                    $("#file_input").val('');
                }
            }
        });


        var advanceset = new Vue({
