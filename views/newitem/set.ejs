                                                    <button type="submit" class="btn btn-red btn+" :disabled="bgmusic.disabled" @click="upload_bgmusic">确定</button>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                        <section class="q-item">
