                    var type = obj.type.split('/')[obj.type.split('/').length-1];
                    if(type!='mp3' && type!='wma' && type!='wav' && type!='ogg' && type!='mpeg'){
                        toast.showmsg('不是音频类型的文件', true);
                        return;
                    };
                    if(obj.size>1572864){
                        toast.showmsg('音频文件不能大于1.5m', true);
                        return;
                    };
                    this.bgmusic.name = obj.name.substr(-20, 20);
                },
                up_shareicon: function(){
                    if(mainWrap.checkexpire())return;
                    if(this.shareicon.url){
                        this.shareicon_change(event,this.shareicon.url);
                    }else{
                        $("#shareicon_input").click();
                    }
                },
                del_shareicon: function(){
                    var _this = this;
                    $.post('/setitem/del_shareicon',{},function(res){
                            if(res.code == 0){
                                toast.showmsg('删除成功');
                                _this.shareicon.type = 0;
                                _this.shareicon.url = '';
                                _this.shareicon.initurl = '';
                            }else{
                                toast.showmsg(res.msg, true);
                            }
                        }
                    );
                },
                shareicon_change: function(event,src){
                    if(src){
                        var url = src;
                    }else{
                        var obj = this.getObject();
                        if(obj.type.split('/')[0]!='image'){
                            toast.showmsg('不是图片类型的文件', true);
                            return;
                        }
                        if(obj.size>51200){
                            toast.showmsg('图片大小不得大于50k', true);
                            return;
                        };
                        var url = obj.url;
                    }
                    this.shareicon.url = obj.url;
                    this.shareicon.name = obj.name;
                },
                cancel_sel_bgmusic: function(){
                    this.bgmusic.name = '';
                    document.getElementById('music_input').value = '';
                },
                upload_bgmusic: function(){
                    this.bgmusic.disabled = true;
                    var _this = this;
                    $.post('/tokenitem/music',{
                            type: "global",
                            name: _this.bgmusic.name
                        },function(res){
                        var token = res.token;
                        var key = res.musicurl;
                        Q.addEvent("progress", function(p, s) {
                            _this.bgmusic.name = (p==100?99:p)+'%';
                        });
                        Q.addEvent("putFinished", function(fsize, res, taking) {
                            _this.bgmusic.disabled = false;
                            _this.bgmusic.name = '';
                            _this.bgmusic.uped = 'http://qncdn.sz-sti.com/'+key;
                            _this.bgmusic.isnew = true;
                            toast.showmsg('上传背景音乐成功');
                        });
                        Q.SetToken(token);
                        Q.Upload(document.getElementById('music_input').files[0], key);
                    });
                },
                cancel_sel_shareicon: function(){
                    this.shareicon.url = '';
                    document.getElementById('shareicon_input').value = '';
                },
                upload_shareicon: function(){
                    this.shareicon.disabled = true;
                    var _this = this;
                    $.post('/tokenitem/shareicon',{name: _this.shareicon.name},function(res){
                        var token = res.token;
                        var key = res.key;
                        var url = res.url;
                        console.log(res);
                        Q.addEvent("progress", function(p, s) {
                            // _this.maskwidth = (p==100?99:p)+'%';
                        });
                        Q.addEvent("putFinished", function(fsize, res, taking) {
                            _this.shareicon.disabled = false;
                            _this.shareicon.url = 'http://qncdn.sz-sti.com/'+url;
                            _this.shareicon.initurl = 'http://qncdn.sz-sti.com/'+url;
                            _this.toggle_shareicon(1);
                            toast.showmsg('设置成功');
                        });
                        Q.SetToken(token);
                        Q.Upload(document.getElementById('shareicon_input').files[0], url);
                    });
                },
                sub_sharedesc: function(){
                    var _this = this;
                    $.post('/setitem/set-sharedesc',{
                            sharead: _this.sharedesc.content.replace(/<[^>]+>/g,"")
                        },function(res){
                            if(res.code == 0)_this.sharedesc.back = _this.sharedesc.content;
                        }
                    )
                },
                cancel_info: function(){
                    this.info.content = this.info.back;
                },
                subinfo: function(){
                    var _this = this;
                    $.post('/setitem/set-global-ad',{
                            advertisement:'<div>'+_this.info.content.ad1+'</div>'+'<div>'+_this.info.content.ad2+'</div>'+'<div>'+_this.info.content.ad3+'</div>'
                        },function(res){
                            // toast.showmsg(res.msg, res.code!=0);
                            if(res.code == 0){
                                _this.info.back.ad1 = _this.info.content.ad1;
                                _this.info.back.ad2 = _this.info.content.ad2;
                                _this.info.back.ad3 = _this.info.content.ad3;
                            }
                        }
                    )
                }
            }
        });
    
        mainWrap.init();
        var body = new Vue({
            el: 'body',
            methods: {
                hidepopt: function(){
                    nav.display = false;
                }
            }
        });
    </script>
</body>
</html>