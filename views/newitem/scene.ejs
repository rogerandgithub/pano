        function checkMobile(){
            if(/AppleWebKit.*Mobile/i.test(navigator.userAgent) 
            || /Android/i.test(navigator.userAgent) 
            || /BlackBerry/i.test(navigator.userAgent) 
            || /IEMobile/i.test(navigator.userAgent) 
            || (/MIDP|SymbianOS|NOKIA|SAMSUNG|LG|NEC|TCL|Alcatel|BIRD|DBTEL|Dopod|PHILIPS|HAIER|LENOVO|MOT-|Nokia|SonyEricsson|SIE-|Amoi|ZTE/.test(navigator.userAgent))){
                if(/iPad/i.test(navigator.userAgent)){
                    return 2;//平板
                }else{
                    return 1;//手机
                }
            }else{
                return 0;
            }
        } 
        function tabsidebarinit(type){
            if(tabsidebar)
                tabsidebar(type);
        }
        function showcommentsinit(type,imgname,imgdes){
            if(showcomments)
                showcomments(type,imgname,imgdes);
        }
        function speechinit(text, i){
            if(speech)
                speech(text, i);
        }
        function openHomeinit(type){
            checkMobile()?location.href="/scene/lists/"+<%=user.id%>:$('#personalHome').show();
        }
        function setCookie(name, value){
            var Days = 30;
            var exp = new Date();
            exp.setTime(exp.getTime() + Days*24*60*60*1000);
            document.cookie = "<%=scenekey?scenekey:prokey%>_"+name+"="+ escape (value) + ";expires=" + exp.toGMTString();
        }
        function getCookie(name){
            var arr,reg=new RegExp("(^| )<%=scenekey?scenekey:prokey%>_"+name+"=([^;]*)(;|$)");
            if(arr=document.cookie.match(reg))
                return unescape(arr[2]);
            else
                return null;
        }
        function like(){
            if(!getCookie('liked_'+sortid)){
                $.post('/scene/like/<%=scenekey%>', {}, function(res){
                    if(res.code!=0){
                        alert(res.msg);
                        return;
                    }
                    setCookie('liked_'+sortid, 1);
                });
            }
        }
        function hideloading(){
            $("#loading").hide();
        }
        function speech(text, i){
            console.log(111)
            i = i?i:'';
            var time = parseInt(text.length/4.5)+4;
            var loading_time = parseInt(text.length*0.003);
            $.get('/scene/baiduapi/vilidatetoken', function(res){
                var data = JSON.parse(res)
                var access_token = data.access_token;
                if (fristflag) {
                    backtext = text;
                    setTimeout(function(){
                        // var src = 'http://tts.baidu.com/text2audio?lan=zh&ie=UTF-8&text=' + text;
                        var src = 'https://tsn.baidu.com/text2audio?tex=' + text + '&ie=UTF-8&lan=zh&cuid=***&ctp=1&tok=' + access_token;
                        $('#audio').attr('src', src);
                    },2500-loading_time);
                    fristflag--;
                    i?closebroadcast(time, i):closebroadcast(time);
                }else{
                    if(backtext!=text){
                        backtext = text;
                        var src = 'https://tsn.baidu.com/text2audio?tex=' + text + '&ie=UTF-8&lan=zh&cuid=***&ctp=1&tok=' + access_token;
                        $('#audio').attr('src', src);
                        i?closebroadcast(time, i):closebroadcast(time);
                    }else{
                        if ($('#audio')[0].paused) {
                            $('#audio')[0].play();
                        }else{
                            $('#audio')[0].pause();
                        }
                    }
                }
            })
        }
        function closebroadcast(time, i){
            var jsstring = i?
                'copy(layer[open_house_broadcast'+i+'].crop,layer[open_house_broadcast'+i+'].crop_closed);set(layer[open_house_broadcast'+i+'].state, "paused");':
                'copy(layer[toggle_broadcast].crop,layer[toggle_broadcast].crop_closed);set(layer[toggle_broadcast].state, "paused");';
            if(krpano)
                setTimeout(function(){
                    krpano.call(jsstring);
                }, time*1000);
        }
        function settitle(){
            var $body = $('body');
            document.title = '<%=user.id==0?sharetitle:title%>';
            var $iframe = $('<iframe style="display:none;" src="/favicon.ico"></iframe>').on('load', function() {
              setTimeout(function() {
                $iframe.off('load').remove()
              }, 0)
            }).appendTo($body)
        }


        // try{
        var trim_Version = navigator.appVersion?(navigator.appVersion.split(";")[1]?navigator.appVersion.split(";")[1].replace(/[ ]/g,""):''):''; 
        // }catch(err){
        //     trim_Version = ''; 
        // }
        var isIE8 = navigator.appName == "Microsoft Internet Explorer" && (trim_Version == "MSIE6.0" || trim_Version == "MSIE7.0" || trim_Version == "MSIE8.0");

        function loadJs(src, fun){

            var script = document.createElement("script");
            script.src = src;
            var s = document.getElementsByTagName("script")[0];

            if(isIE8){
                script.onreadystatechange = function(){
                    var r = script.readyState; 
                    if (r === 'loaded' || r === 'complete') { 
                        // script.onreadystatechange = null; 
                        fun(); 
                    }
                }
            }else{
                script.onload = function(){
                    fun();
                }
            }
            s.parentNode.insertBefore(script, s);
        };

