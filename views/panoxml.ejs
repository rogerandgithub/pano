<krpano version="1.19" onstart="startup();" showerrors="false">

    <include url="/skin/vtourskin<%if(jz.is_jz){%>_jz<%}%>.xml?v=6" />

    <!-- set skin settings: bingmaps? gyro? thumbnails? tooltips? -->
    <skin_settings maps="false"
                   maps_zoombuttons="false"
                   gyro="true"
                   title="true"
                   thumbs="true"
                   thumbs_width="122" thumbs_height="82" thumbs_padding="16" thumbs_crop="0|40|250|210"
                   thumbs_opened="false"
                   thumbs_text="true"
                   littleplanetintro="true"
                   thumbs_dragging="true"
                   thumbs_onhoverscrolling="true"
                   thumbs_scrollbuttons="false"
                   thumbs_scrollindicator="false"
                   thumbs_loop="false"
                   tooltips_thumbs="true"
                   tooltips_hotspots="false"
                   tooltips_mapspots="false"
                   loadscene_flags="MERGE"
                   loadscene_blend="NOBLEND"
                   controlbar_offset="40"
                   background_music="<%=bgmusic%>"
                   bgmusicstatus="<%=!!owner.permission_bgsnd%>"
                   telephone_state="<%=!!telephone%>"
                   maps_state="<%=!!maps[0]%>"
                   telephone="tel:<%=telephone%>"
                   broadcast_init="<%=!!owner.permission_introduction%>"
                   autocircle="<%=autoplay!=0%>"
                   map_init="false"
                   menu_name="<%=company%>"
                   menu_url="http://<%=itcwebsite%>"
    />

    <!-- set optional skin logo url -->
    <layer name="skin_logo" url="" scale="0.25" opened_onclick="openurl('...',_blank);" />
    <action name="startup">
        <%if(owner.permission_bgsnd&&preview!=2){%>playsound(bgsnd,'<%=bgmusic%>',0);<%}%>
        if(device.mobile, set(stagescale,0.5););
        if(startscene === null, copy(startscene,scene[0].name));
        set(control.mousetype, drag2d);
        copy(isrotate, true);

        if(rotate == no || <%=rotate=='no'%>, set(layer[toggle_rotate].state, 'paused');,  set(layer[toggle_rotate].state, 'opened'););

        loadscene(get(startscene), null, MERGE); 
        <%allviews=0;for(var i in romaing){allviews+=romaing[i].visited}%>

        <%if(jz.is_jz){%>
        set(modetxt, mode);
        set(ismode, false);
        getarr(<%=jz.s_obj[jz.arr[0]]%>);
        <%}%>
    </action>

    <style name="btngroup" url="http://qncdn.sz-sti.com/images/btngroup.png?v=5" visible="false" />




    <%if(lists.length>0){%>
    <style name="rec_thumb_style" align="left" edge="center" width="100" height="66" />

    <layer name="rec_thumbs_container" keep="true" type="container" align="leftbottom" y="80" width="100%" height="80">
        <layer name="rec_scroll_container" keep="true" type="container" align="leftbottom" y="-100%" width="100%" height="100%" bgcolor="0x000000" bgalpha="0.3">
            <layer name="rec_thumbs" state="closed" url="http://qncdn.sz-sti.com/document/plugins/scrollarea.swf" alturl="http://qncdn.sz-sti.com/document/plugins/scrollarea.js" align="center" width="<%=230+(lists.length-1)*110%>" height="100%" direction="h" onhover_autoscrolling="true" onloaded="rec_thumbs_init">
                <%for(var i in lists){%>
                <layer name="thumb<%=1+parseInt(i)%>" url="http://qncdn.sz-sti.com/images/scenes/<%=lists[i].dataValues.sceneKey%>/allinone.jpg?imageMogr2/gravity/NorthWest/crop/!1000x660a0a800/thumbnail/!10p" style="rec_thumb_style" x="<%=i*110+60%>" onclick="hit(<%=lists[i].dataValues.sceneKey%>)"/>
                <%}%>
                <layer name="thumb<%=2+parseInt(i)%>" url="/images/morebg.jpg" style="rec_thumb_style" x="<%=i*110+170%>" onclick="openurl('/scene/lists/<%=owner.id%>',_blank);"/>
            </layer>
        </layer>
    </layer>

    <layer name="rec_thumbborder" parent="thumb<%=2+parseInt(i)%>" keep="true" url="/skin/vtourskin_thumbborder.png" enabled="false" align="lefttop" width="100%" height="100%" />

    <action name="hit">
        ifnot(sc1, copy(sc1, scale););
        mul(sc09, scale, 0.9);
        tween(scale, get(sc09), 0.1, default, tween(scale, get(sc1), 0.1, easeOutBounce));
        ifnot(%1 == null, txtadd(surl, 'http://wx.sz-sti.com/scene?key=', %1);openurl(get(surl),_blank););
    </action>

    <action name="rec_showthumbs">
        if(%1 == null, if(layer[rec_thumbs].state == 'closed', set(show,true), set(show,false)); , set(show,%1); );
        mul(rech, layer[rec_scroll_container].pixelheight, -1);
        if(layer[rec_scroll_container].y == -100%, copy(layer[rec_scroll_container].y,mh));
        if(show,
            set(layer[rec_thumbs].state, 'opened');
            tween(layer[rec_scroll_container].y, 0, 0.5, easeOutQuint);
            set(layer[rec_scroll_container].visible, true);
            skin_showthumbs(false);
          ,
            set(layer[rec_thumbs].state, 'closed');
            tween(layer[rec_scroll_container].y, get(rech), 0.5, easeOutQuint, set(layer[rec_scroll_container].visible, false););
        );
    </action>

    <action name="rec_thumbs_init">
        
        for(set(i,0);set(j,0), j LT layer.count, inc(j), if(layer[get(j)].parent == 'rec_thumbs', inc(i)); );
        for(set(k,1), k LE i, inc(k),
            if(skin_settings.thumbs_text,
                txtadd(rec_thumbname, 'thumb', get(k));
                txtadd(rec_thumbtext, 'rec_thumbtext_', get(k));
                txtadd(thumbtitle, '推荐房源', get(k));
                addlayer(get(rec_thumbtext));
                layer[get(rec_thumbtext)].loadstyle(skin_thumbtext_style);
                set(layer[get(rec_thumbtext)].keep, true);
                set(layer[get(rec_thumbtext)].parent, get(rec_thumbname));
                set(layer[get(rec_thumbtext)].html, get(thumbtitle));
            );
        );
        set(layer[get(rec_thumbtext)].html, '查看更多');
        if(device.mobile, mul(width, 1.8););
        setcenter(0,0);
    </action>
    <%}%>




    //<style name="set_bar_btn" url="/images/newBtn.png" align="top" width="40" height="40"  />
    <style name="set_bar_btn" url="/images/leftnavbar.png?v=1" align="top" width="50" height="50" />

    <style name="set_bar_text" align="bottom" width="100%" handcursor="true" url="http://qncdn.sz-sti.com/document/plugins/textfield.swf" autoheight="true" y="5" enabled="false" background="false" border="false" css="text-align:center; font-family:黑体; color:#EEEEEE; font-family:黑体; font-size:12px;" textshadow="1"  />

    <style name="maps_link" url="http://qncdn.sz-sti.com/images/tool/sprites.png" crop="73|196|50|50" keep="true" width="7%" height="prop" align="lefttop" />

    <style name="maps_link_text" css="text-align:center; color:#eee; font-family:黑体; font-size:14px;" onloaded="txtadd(tooltip, '跳转至 ', get(html));" autoheight="true" handcursor="true" background="false" textshadow="1" />

    <style name="maps_link_to" onclick="if(linkedscene, skin_hidetooltips(); looktohotspot(); loadscene(get(linkedscene),null,get(skin_settings.loadscene_flags),get(skin_settings.loadscene_blend)); skin_updatescroll();callwith(layer[open_house_maps], onclick););callwith(layer[toggle_map], onclick);" />


    //toast common
    <layer name="toast_text" keep="true"
           url="http://qncdn.sz-sti.com/document/plugins/textfield.swf"
           align="center" edge="bottom" oy="-2" autoheight="true"
           visible="false" alpha="0"
           background="true" backgroundcolor="0x000000" backgroundalpha="0.5"
           border="true" bordercolor="0xFFFFFF" borderalpha="1" borderwidth="2" roundedge="4"
           shadow="0.0" shadowrange="4.0" shadowangle="45" shadowcolor="0x000000" shadowalpha="1.0"
           textshadow="1" textshadowrange="6.0" textshadowangle="90" textshadowcolor="0x000000" textshadowalpha="1.0"
           css="text-align:center; margin:5px 10px;; color:#FFFFFF; line-height:26px; font-family:黑体; font-weight:500; font-size:14px;overflow: hidden;"
           html=""
           />


    <%if(preview!=2 && preview!=3){%>
