    <div id="sti-wrapper">
        <a href="http://www.robosense.cn/web/cn" class="sti-btn"></a>
    </div>

    <script type="text/javascript">
        window.onload = function(){
            setTimeout(function(){
                location.href = 'http://www.robosense.cn/web/cn';
            },4000);
        }
    </script>
