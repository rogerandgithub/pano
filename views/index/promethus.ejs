<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="robots" content="noarchive">
    <meta name="format-detection" content="telephone=no" />
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <title><%=lang=='cn'?'HAD激光雷达感知方案-感知算法':'RS-LiDAR Products'%></title>
    <link rel="stylesheet" href="/index/css/common.css" type="text/css"> 
    <style type="text/css">
        #backtop{
            display: none;
        }
        <%if(lang=='en'){%>
            .buy-box a {
                display: block;
                width: 110px;
            }
        <%}%>
        html, body{
            width: 100%;
            height: 100%;
            overflow: hidden;
        }
        #rslidar{
            width: 100%;
            height: 100%;
        }
        #rslidar .content-warp {
            width: 100%;
            height: 100%;
            background: url('/index/images/main/puluonixiusi.jpg') no-repeat center bottom;
            background-size: cover;
            position: relative;
        }
        #rslidar .content-warp .fade{
            position: absolute;
            width: 60px;
            left: 50%;
            bottom: 30px;
            transform: translate3d(-50%,0,0);
            -webkit-transform: translate3d(-50%,0,0);
            -moz-transform: translate3d(-50%,0,0);
            -ms-transform: translate3d(-50%,0,0);
            animation: fade 1.8s infinite;
            -webkit-animation: fade 1.8s infinite;
            -moz-animation: fade 1.8s infinite;
            -ms-animation: fade 1.8s infinite;
        }
        #rslidar .content-warp .fade img{
            display: block;
            width: 100%;
        }
        @keyframes fade{
            0% {
                opacity: 0;
            }
            50% {
                opacity: 1;
            }
            100% {
                opacity: 0;
            }
        }
        @-webkit-keyframes fade{
            0% {
                opacity: 0;
            }
            50% {
                opacity: 1;
            }
            100% {
                opacity: 0;
            }
        }
        @-moz-keyframes fade{
            0% {
                opacity: 0;
            }
            50% {
                opacity: 1;
            }
            100% {
                opacity: 0;
            }
        }
        @-ms-keyframes fade{
            0% {
                opacity: 0;
            }
            50% {
                opacity: 1;
            }
            100% {
                opacity: 0;
            }
        }
        .flexslider .slides li.planBox .fade img{
            display: block;
            width: 100%;
        }
    </style>
</head>
<body>
    <%include nav%>
    <%include footer%>
    <div id="navtop">
        <navtop>
          <div class="three-menu" slot="navbottom">
            <div class="link">
                <a href="/web/rslidar/<%=lang%>" class="button">
                    <span v-text="threeMenu.rslidar"></span>
                </a>
            </div>
            <div class="link">
                <a href="/web/rslidar/platform/<%=lang%>" class="button">
                    <span v-text="threeMenu.platform"></span>
                </a>
            </div>
            <div class="link">
                <a href="/web/rslidar/algorithm/<%=lang%>" class="button">
                    <span v-text="threeMenu.algorithm"></span>
                </a>
            </div>
            <div class="link">
                <a href="/web/rslidar/promethus/<%=lang%>" class="button active">
                    <span v-text="threeMenu.prometheus"></span>
                </a>
            </div>
          </div>
        </navtop>
    </div>
    <div id="rslidar">
        <div class="content-warp">
            <div class="fade">
                <a href="/web/prometheus/cn"><img src="/index/images/main/fade.png" draggable="false"></a>
            </div>
        </div>
    </div>
    
    <script src="/index/js/jquery-2.2.4.min.js"></script>
    <script src="/index/js/aos.js"></script>
    <script src="/index/js/smooth-scroll.min.js"></script>
    <script type="text/javascript">
    </script>
    <script type="text/javascript">
        var navtop = new Vue({
            el: '#navtop',
            data: {
                threeMenu: ''
            }
        });
        var footnav = new Vue({
            el: '#footnav'
        });
        var rslidar = new Vue({
            el: '#rslidar',
            data: {
                datas: <%-include("lang_"+lang, {lang: lang})%>
            },
            methods: {
                init: function(){
                    navtop.threeMenu = this.datas.rslidarIntro.threeMenu;
                    // AOS.init({
                    //     easing: 'ease-out-back',
                    //     duration: 1000
                    // });
                }
            }
        });
        rslidar.init();
    </script>
</body>
</html>