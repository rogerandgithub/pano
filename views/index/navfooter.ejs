#footer .arlist2 li:nth-of-type(3){background: url(http://qncdn.sz-sti.com/images/three-dimensional-product/telphone.png) 0 0px no-repeat;background-size: 18px;}
#footer .arlist2 .wechat{width: 24px;height: 24px;background: url(http://qncdn.sz-sti.com/images/three-dimensional-product/wechat.png) no-repeat;position: relative;top: 3px;cursor: pointer;}
#footer .arlist2 .wechat img{display: block;position: absolute;width: 175px;bottom: 30px;right: 0px;}
#footer .arlist li a,#footer .arlist1 li a {color: #cfcdcd;}
#footer .compyright {width: 100%;line-height: 30px;padding: 20px 0;border-top: 1px solid #cfcdcd;clear: both;}
