/*--------------------topNav----------------------*/
#navtop{position: fixed;left: 0;top: 0;width: 100%;height: 159px;z-index: 99;z-index: 9999;transition: all 0.6s;-webkit-transition: all 0.6s;-ms-transition: all 0.6s;-moz-transition: all 0.6s;}
.black{background: #000;}
#header{position: relative;}
.white{background-color: transparent !important;}
#nav {margin:0 auto; width:1000px;height: 69px; display:block;}
#nav .logo {float:left;display:block;margin-right: 100px;margin-top: 13px;}
#nav .logo a {display:block;}
#nav .mainlevel {float:left;font-size: 16px;position: relative;}
#langlist{position: absolute;width: 100%;background-color: #000;z-index: 9999;}
#langlist a.lang{height: 42px;line-height: 42px !important;color:#fff;cursor: pointer;text-align: center;z-index: 9999;transition: all 0.6s;-webkit-transition: all 0.6s;-moz-transition: all 0.6s;-ms-transition: all 0.6s; }
#langlist a.lang:hover{color:#e00024 !important;}
#nav .mainlevel a {text-decoration:none; line-height:32px; display:block; padding:0 20px;color: #fff;line-height: 69px;outline-offset: 0;}
#header .three-menu{width: 100%;height: 90px;overflow:hidden;background:#fff;text-align: center;border-bottom: 1px solid #eee;box-shadow: 0 3px 10px rgba(0,0,0,0.3);}
#header .three-menu .link{display:inline-block;cursor: pointer;margin:23px 58px;position:relative;}
#header .three-menu .button{display:block;width:160px;height:40px;border:2px solid #807f7f;line-height:40px;font-size:16px;color:#807f7f;-moz-transition:ease 0.4s;-o-transition:ease 0.4s;-webkit-transition:ease 0.4s;transition:ease 0.4s;position:relative;}
#header .three-menu a.active{border:2px solid #df0024;color: #df0024;}
#header .three-menu .button:hover{border:2px solid #df0024;}
#header .three-menu .button .line{position:absolute;background:none; -moz-transition:ease 0.4s;-o-transition:ease 0.4s;-webkit-transition:ease 0.4s;transition:ease 0.4s;}
#header .three-menu .button:hover .line{background:#df0024;}
#header .three-menu .button .line-top{width:0px;height:2px;left:-110%;top:-2px;}
#header .three-menu .button:hover .line-top{width:100%;left:-2px;}
#header .three-menu .button .line-right{width:2px;height:0px;right:-2px;top:-110%;}
#header .three-menu .button:hover .line-right{height:100%;top:-2px;}
#header .three-menu .button .line-bottom{width:2px;height:0px;left:-2px;bottom:-110%;}
#header .three-menu .button:hover .line-bottom{height:100%;bottom:-2px;}
#header .three-menu .button .line-left{width:0px;height:2px;right:-110%;bottom:-2px;}
#header .three-menu .button:hover .line-left{width:100%;right:-2px;}
#header .three-menu a:hover{border:2px solid #df0024;color: #df0024;}
/*--------------------footer----------------------*/
#footer {width: 100%;height: 400px;color: #cfcdcd;background-color: #252525;}
#footer .w {width: 1000px;margin: 0 auto;height: auto;}
#footer .cls {clear: both;}
#footer .cls a{cursor: pointer;}
#footer .arlist {width: 130px;float: left;margin-right: 110px;margin-top: 40px;}
#footer a{transition: all 0.6s;-webkit-transition: all 0.6s;-moz-transition: all 0.6s;-ms-transition: all 0.6s;}
#footer .arlist a:hover,#footer .arlist1 a:hover{color:#e00024;}
#footer .arlist1 {width: 170px;float: left;margin-right: 110px;margin-top: 40px;}
#footer .arlist2 {width: 480px;float: left;margin-top: 40px;margin-bottom: 120px;}
#footer .arlist h2,#footer .arlist1 h2, #footer .arlist2 h2 {border-bottom: 2px solid #cfcdcd;font-weight: normal;font-size: 20px;color: #e9e5e5;line-height: 35px;margin-bottom: 15px;}
#footer .arlist li,#footer .arlist1 li {float: left;width: 100%;line-height: 24px;font-size: 14px;}
#footer .arlist2 li {max-width: 100%;line-height: 24px;font-size: 14px;padding-left: 28px;word-wrap: break-word;box-sizing: border-box;-webkit-box-sizing: border-box;cursor: pointer;}
#footer .arlist2 li:nth-of-type(1){background: url(http://qncdn.sz-sti.com/images/three-dimensional-product/address.png) 0 2px no-repeat;background-size: 18px;}
