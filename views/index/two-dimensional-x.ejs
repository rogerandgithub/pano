    <script type="text/javascript">
        $(function(){
            var timer,flag = true;
            function lock(){
                clearTimeout(timer);
                flag = false;
                timer = setTimeout(function(){
                    flag= true;
                }, 200);                
            }
            $('#two-dimensional').fullpage({
                loopBottom : true,        // 顶部轮滚
                // loopTop : true,           // 顶部轮滚
                css3 : true,              // 开启CSS3动画
                scrollingSpeed : 1500,     //页面滚动速度，单位为毫秒
                easing : 'easeIn',        //滚动动画方式
                keyboardScrolling : true, //键盘控制
                navigation : true,        //显示项目导航
                 
                onLeave : function( index,nextIndex,direction ){ 
                    //滚动前的回调函数,index  是离开的“页面”的序号，从1开始计算；nextIndex 是滚动到的“页面”的序号，从1开始计算；direction 判断往上滚动还是往下滚动，值是 up 或 down
                    if( direction=='down' ){
                      move('#header').y(-69).duration('1s').end();
                    }
                    if( direction=='up' ){
                      move('#header').y(0).duration('1s').end();
                    }
                    var robosense = $("#two-dimensional-x");
                    if ( index===1 && nextIndex===3 ){
                        // robosense.addClass("header");
                        move('#header').y(0).duration('1s').end();
                        return false;
                    }
                    // 底部
                    if ( index===3 && nextIndex===1 ){
                        robosense.addClass("footer");
                        move('#header').y(-69).duration('1s').end();
                        return false;
                    }
                    if ( index===3 && nextIndex===2 && robosense.hasClass("footer") ){
                        lock();
                        robosense.removeClass("footer");
                        return false;
                    }
                    // 返回事件阻塞
                    return flag;
                },
                afterRender : function(){
                    //页面初始化完成后的回调函数
                    // move('#header').y(-69).duration('1s').end();
                    // var robosense = $("#robosense");
                    // robosense.addClass("header");
                    return false;
                }
            })    
        })
    </script>
