    <script type="text/javascript" src="/index/js/move.js"></script>
    <script type="text/javascript">
        $(function(){
            var timer,flag = true;
            var bottomOff = false;
            var haha = false;
            function lock(){
                clearTimeout(timer);
                flag = false;
                timer = setTimeout(function(){
                    flag= true;
                }, 200);                
            }
            $('#two-dimensional').fullpage({
                loopBottom : true,        // 顶部轮滚
                //loopTop : true,           // 顶部轮滚
                css3 : true,              // 开启CSS3动画
                scrollingSpeed : 1500,     //页面滚动速度，单位为毫秒
                easing : 'easeIn',        //滚动动画方式
                keyboardScrolling : true, //键盘控制
                navigation : true,        //显示项目导航
                navigationTooltips : ['简介', '优势', '优势', '应用','参数'],
                 
                onLeave : function( index,nextIndex,direction ){ 
                    //滚动前的回调函数,index  是离开的“页面”的序号，从1开始计算；nextIndex 是滚动到的“页面”的序号，从1开始计算；direction 判断往上滚动还是往下滚动，值是 up 或 down
                    if( direction=='down'){
                      move('#header').y(-69).duration('1s').end();
                    }
                    if( direction=='up'){
                      move('#header').y(0).duration('1s').end();
                    }
                    var robosense = $("#two-dimensional-product");
                    if( nextIndex===1 ){
                      move('.page_1 .section_text').x(0).set('opacity',1).duration('2s').end();
                      move('.page_1 .section_pic').x(0).set('opacity',1).duration('2s').end();
                      move('.page_2 .section_pic').x(100).set('opacity',0).duration('1s').end();

                      move('.page_2 .section_text .title').x(-100).set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic1').set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic2').set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic3').set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic4').set('opacity',0).duration('1s').end();

                      move('.page_3 .title').scale(0).duration('1s').end();
                      move('.page_3 .info').set('opacity',0).duration('1s').end();

                      move('.page_4 h2').scale(0).duration('1s').end();
                      move('.page_4 .sceneList .pic_1').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_2').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_3').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_4').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_5').rotate(360).duration(400).end();

                      move('.page_5 .section_text').x(-100).duration('1s').end();
                      move('.page_5 .section_pic').x(100).duration('1s').end();

                      robosense.removeClass("footer");
                    }
                    if( nextIndex===2 ){
                      move('.page_1 .section_text').x(100).set('opacity',0).duration('1s').end();
                      move('.page_1 .section_pic').x(-100).set('opacity',0).duration('1s').end();

                      move('.page_2 .section_pic').x(0).set('opacity',1).duration('2s').end();
                      move('.page_2 .section_text .title').x(0).set('opacity',1).duration('2s').end();
                      move('.page_2 .fourbox .pic1').delay(600).set('opacity',1).duration('1s').end();
                      move('.page_2 .fourbox .pic2').delay(800).set('opacity',1).duration('1s').end();
                      move('.page_2 .fourbox .pic3').delay(1000).set('opacity',1).duration('1s').end();
                      move('.page_2 .fourbox .pic4').delay(1200).set('opacity',1).duration('1s').end();

                      move('.page_3 .title').scale(0).duration('1s').end();
                      move('.page_3 .info').set('opacity',0).duration('1s').end();

                      move('.page_4 h2').scale(0).duration('1s').end();
                      move('.page_4 .sceneList .pic_1').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_2').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_3').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_4').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_5').rotate(360).duration(400).end();

                      move('.page_5 .section_text').x(-100).duration('1s').end();
                      move('.page_5 .section_pic').x(100).duration('1s').end();

                      robosense.removeClass("footer");
                    }
                    if( nextIndex===3 ){
                      move('.page_1 .section_text').x(100).set('opacity',0).duration('1s').end();
                      move('.page_1 .section_pic').x(-100).set('opacity',0).duration('1s').end();

                      move('.page_2 .section_pic').x(100).set('opacity',0).duration('1s').end();
                      move('.page_2 .section_text .title').x(-100).set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic1').set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic2').set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic3').set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic4').set('opacity',0).duration('1s').end();

                      move('.page_3 .title').scale(1).duration('2s').end();
                      move('.page_3 .info').delay(400).set('opacity',1).duration('2s').end();

                      move('.page_4 h2').scale(0).duration('1s').end();
                      move('.page_4 .sceneList .pic_1').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_2').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_3').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_4').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_5').rotate(360).duration(400).end();

                      move('.page_5 .section_text').x(-100).duration('1s').end();
                      move('.page_5 .section_pic').x(100).duration('1s').end();

                      robosense.removeClass("footer");
                    }
                    if( nextIndex===4 ){
                      if( index!==5 ){
                        move('.page_4 h2').scale(1).duration('2s').end();
                        move('.page_4 .sceneList .pic_1').delay(500).rotate(0).duration(400).end();
                        move('.page_4 .sceneList .pic_2').delay(700).rotate(0).duration(400).end();
                        move('.page_4 .sceneList .pic_3').delay(900).rotate(0).duration(400).end();
                        move('.page_4 .sceneList .pic_4').delay(1100).rotate(0).duration(400).end();
                        move('.page_4 .sceneList .pic_5').delay(1300).rotate(0).duration(400).end();
                      }
                      move('.page_1 .section_text').x(100).set('opacity',0).duration('1s').end();
                      move('.page_1 .section_pic').x(-100).set('opacity',0).duration('1s').end();

                      move('.page_2 .section_pic').x(100).set('opacity',0).duration('1s').end();
                      move('.page_2 .section_text .title').x(-100).set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic1').set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic2').set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic3').set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic4').set('opacity',0).duration('1s').end();

                      move('.page_3 .title').scale(0).duration('1s').end();
                      move('.page_3 .info').set('opacity',0).duration('1s').end();

                      if( index===5 && bottomOff ){
                        move('.page_5 .section_text').x(0).duration('2s').end();
                        haha = true;
                        move('.page_5 .section_pic').x(0).duration('2s').end(function(){
                          haha = false;
                        });
                        lock();
                        robosense.removeClass("footer");
                        bottomOff = false;
                        return false;
                      }else if( index===5 && !bottomOff ){
                        if(!haha){
                          move('.page_4 h2').scale(1).duration('2s').end();
                          move('.page_4 .sceneList .pic_1').delay(500).rotate(0).duration(400).end();
                          move('.page_4 .sceneList .pic_2').delay(700).rotate(0).duration(400).end();
                          move('.page_4 .sceneList .pic_3').delay(900).rotate(0).duration(400).end();
                          move('.page_4 .sceneList .pic_4').delay(1100).rotate(0).duration(400).end();
                          move('.page_4 .sceneList .pic_5').delay(1300).rotate(0).duration(400).end();

                          move('.page_5 .section_text').x(-100).duration('1s').end();
                          move('.page_5 .section_pic').x(100).duration('1s').end();
                        }
                      }
                      return flag;
                    }
                    if( nextIndex===5 ){
                      move('.page_5 .section_text').x(0).duration('2s').end();
                      move('.page_5 .section_pic').x(0).duration('2s').end();

                      move('.page_1 .section_text').x(100).set('opacity',0).duration('1s').end();
                      move('.page_1 .section_pic').x(-100).set('opacity',0).duration('1s').end();

                      move('.page_2 .section_pic').x(100).set('opacity',0).duration('1s').end();
                      move('.page_2 .section_text .title').x(-100).set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic1').set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic2').set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic3').set('opacity',0).duration('1s').end();
                      move('.page_2 .fourbox .pic4').set('opacity',0).duration('1s').end();

                      move('.page_3 .title').scale(0).duration('1s').end();
                      move('.page_3 .info').set('opacity',0).duration('1s').end();

                      move('.page_4 h2').scale(0).duration('1s').end();
                      move('.page_4 .sceneList .pic_1').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_2').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_3').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_4').rotate(360).duration(400).end();
                      move('.page_4 .sceneList .pic_5').rotate(360).duration(400).end();

                    }
                    // 底部
                    if ( index===5 && nextIndex===1 ){
                        move('.page_5 .section_text').x(0).duration('2s').end();
                        move('.page_5 .section_pic').x(0).duration('2s').end();

                        move('.page_4 h2').scale(0).duration('1s').end();
                        move('.page_4 .sceneList .pic_1').rotate(360).duration(400).end();
                        move('.page_4 .sceneList .pic_2').rotate(360).duration(400).end();
                        move('.page_4 .sceneList .pic_3').rotate(360).duration(400).end();
                        move('.page_4 .sceneList .pic_4').rotate(360).duration(400).end();
                        move('.page_4 .sceneList .pic_5').rotate(360).duration(400).end();

                        robosense.addClass("footer");
                        bottomOff = true;
                        return false;
                    }
                    // 返回事件阻塞
                    return flag;
                },
                afterRender : function(){
                    //页面初始化完成后的回调函数
                    // move('#header').y(-69).duration('1s').end();
                    // var robosense = $("#robosense");
                    // robosense.addClass("header");
                    move('.page_1 .section_text').x(0).set('opacity',1).duration('2s').end();
                    move('.page_1 .section_pic').x(0).set('opacity',1).duration('2s').end();
                    return false;
                }
            })    
        })
    </script>
