<krpano version="1.18" title="成都市武侯区玉林" showerrors="false">

    <include url="/skin/vtourskin.xml" />

    <!-- set skin settings: bingmaps? gyro? thumbnails? tooltips? -->
    <skin_settings maps="false"
                   maps_zoombuttons="false"
                   gyro="true"
                   title="true"
                   thumbs="true"
                   thumbs_width="122" thumbs_height="82" thumbs_padding="10" thumbs_crop="0|40|250|210"
                   thumbs_opened="true"
                   thumbs_text="true"
                   thumbs_dragging="true"
                   thumbs_onhoverscrolling="true"
                   thumbs_scrollbuttons="false"
                   thumbs_scrollindicator="false"
                   thumbs_loop="false"
                   tooltips_thumbs="true"
                   tooltips_hotspots="false"
                   tooltips_mapspots="false"
                   loadscene_flags="MERGE"
                   loadscene_blend="BLEND(0.5)"
                   controlbar_offset="20"
    />

    <!-- set optional skin logo url -->


    <!-- 底部广告图 -->
    <hotspot name="bottom_background"
             url="http://qncdn.sz-sti.com/images/tools/bottom.png"
             enabled="true"
             keep="true"
             visible=""
             handcursor="false"
             ath="0" atv="90" rx="0.0" ry="0.0" rz="0.0"
             width="510" height="510"
             distorted="true"
             ondown="skin_view_change_with_hotspot();"
             />
    <hotspot name="bottom_trademark"
             url="%SWFPATH%/plugins/textfield.swf"
             css="text-align:center; color:#494949; font-family:黑体; text-align:center;  font-size:40px;"
             enabled="true"
             keep="true"
             shadowangle="45" shadowrange="4.0"
             handcursor="false"
             background="false"
             ath="0" atv="90" rx="0.0" ry="0.0" rz="0.0"
             distorted="true"
             html="1"
             />

    <!-- 顶部广告图 -->
    <hotspot name="top_trademark" 
             type="image"
             url="http://test.haofang.net:20001/Test/images/icon_2.jpg"
             waittime="1"
             keep="true"
             devices="all"
             handcursor="false"
             ath="0" atv="-90" rx="0.0" ry="0.0" rz="0.0"
             width="360" height="360"
             distorted="true"
             ondown="skin_view_change_with_hotspot();"
             />

